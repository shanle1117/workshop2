{
  "name": "@shanle1117/faix-chatbot",
  "version": "1.0.1",
  "description": "FAIX Chatbot with React and react-markdown",
  "main": "frontend/dist/chatbot-react.js",
  "files": [
    "frontend/dist",
    "frontend/style.css",
    "src/react",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "chatbot",
    "react",
    "faix",
    "ai",
    "conversational-ai"
  ],
  "author": "shanle1117",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanle1117/workshop2.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "start": "webpack serve --mode development",
    "publish:github": "npm publish",
    "test-auth": "npm whoami --registry=https://npm.pkg.github.com",
    "setup:github": "node scripts/check-github-auth.js",
    "release": "node scripts/create-release.js",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
